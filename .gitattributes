# Auto detect text files and perform LF normalization
* text=auto

*.json linguist-detectable=true
*.json linguist-vendored=false
*.json linguist-generated=false
*.json linguist-documentation=false

*.sqlite linguist-detectable=true
*.sqlite linguist-vendored=false
*.sqlite linguist-generated=false
*.sqlite linguist-documentation=false

*.md linguist-detectable=true
*.md linguist-vendored=false
*.md linguist-generated=false
*.md linguist-documentation=false

*.db linguist-detectable=true
*.db linguist-vendored=false
*.db linguist-generated=false
*.db linguist-documentation=false
